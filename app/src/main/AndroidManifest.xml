<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="cn.studyjams.s1.sj61.luolaiyuan">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".NavigatieToSchool"
            android:label="@string/navigation_title"></activity>
        <activity
            android:name=".EatingAtSchool"
            android:label="@string/eating_title"></activity>
        <activity
            android:name=".LeaveAtSchool"
            android:label="@string/living_title"></activity>
        <activity
            android:name=".LearningAtSchool"
            android:label="@string/learn_title"></activity>
        <activity
            android:name=".OfficialWebSite"
            android:label="@string/official_webSite_title"></activity>
        <activity
            android:name=".AnswerQuestion"
            android:label="@string/answer_title"></activity>
        <activity
            android:name=".AnswerQuestionDetails"
            android:label="@string/answer_title"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity"/>
        </activity>
    </application>

</manifest>